## HG Alias
alias ap='hg sync'
alias hga='hg amend'
alias hgb='hg book'
alias hgc='hg commit'
alias hgca='hg commit --amend'
alias hgd='hg diff'
alias hgn='hg next'
alias hgp='hg prev'
alias hgrbd='hg rebase -s . -d'
alias hgrbm='hg rebase -d master'
alias hgrbs='hg rebase -d . -s'
alias hgrbc='hg rebase --continue'
alias hgrst='hg update . --clean'
alias hgs="hg xl"
alias hglg="hg ll"

# Information for single commit
alias hgss='hg log -r . --stat'
alias hgsh='hg log -pr .'
alias hgst='hg status'
